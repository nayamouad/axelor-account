<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<object-views xmlns="http://apps.axelor.com/xml/ns/object-views"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://apps.axelor.com/xml/ns/object-views http://apps.axelor.com/xml/ns/object-views/object-views_0.9.xsd">
    <grid name="matrix-structure-grid" title="Pricing list masks" model="com.axelor.apps.account.db.MatrixStructure">
        <field name="name"/>
        <field name="nbEntry"/>
        <field name="nbRelease"/>
    </grid>
    <form name="matrix-structure-form" title="Pricing list mask" model="com.axelor.apps.account.db.MatrixStructure" cols="6">
        <field name="name"/>
        <field name="nbEntry"/>
        <field name="nbRelease"/>
        <notebook colSpan="6">
            <page title="Parameters" cols="4">
                <group title="Sources" colSpan="4">
                     <field name="sourceParam1"/>
                     <field name="param1"/>
                     <field name="sourceParam2"/>
                     <field name="param2"/>
                     <field name="sourceParam3"/>
                     <field name="param3"/>
                     <field name="sourceParam4"/>
                     <field name="param4"/>
                     <field name="sourceParam5"/>
                     <field name="param5"/>
                     <field name="sourceParam6"/>
                     <field name="param6"/>
                     <field name="sourceParam7"/>
                     <field name="param7"/>
                     <field name="sourceParam8"/>
                     <field name="param8"/>
                    <group title="Range" colSpan="4">
                        <field name="sourceParam9"/>
                        <break/>
                        <field name="param9Incl"/>
                        <field name="param9"/>
                        <field name="param10Incl"/>
                        <field name="param10"/>
                    </group>
                </group>
            </page>
            <page title="Values" cols="4">
                <group title="Nom" colSpan="2">
                    <field name="valueName1" colSpan="4"/>
                    <field name="valueName2" colSpan="4"/>
                    <field name="valueName3" colSpan="4"/>
                    <field name="valueName4" colSpan="4"/>
                    <field name="valueName5" colSpan="4"/>
                </group>
                <group title="Code" colSpan="2">
                    <field name="valueCode1" colSpan="4"/>
                    <field name="valueCode2" colSpan="4"/>
                    <field name="valueCode3" colSpan="4"/>
                    <field name="valueCode4" colSpan="4"/>
                    <field name="valueCode5" colSpan="4"/>
                </group>
            </page>
        </notebook>
    </form>
</object-views>